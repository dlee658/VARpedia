<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <MediaView fx:id="mediaPlayer" fitHeight="300.0" fitWidth="600.0" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <HBox prefHeight="42.0" prefWidth="600.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="playBtn" mnemonicParsing="false" text="Play" />
            <Slider prefHeight="14.0" prefWidth="350.0" />
            <Label fx:id="timeLabel" text="time" />
            <Label fx:id="volumeLabel" text="Volume">
               <padding>
                  <Insets left="30.0" />
               </padding>
            </Label>
            <Slider fx:id="volSlider" prefWidth="10.0" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
   <top>
      <HBox prefHeight="39.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="Back">
               <HBox.margin>
                  <Insets left="10.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
</BorderPane>
